{
    "name": "zeropingheroes/lanager",
    "description": "A LAN party management web application",
    "authors": [
        {
            "name": "Olly Baker",
            "email": "ilumos@gmail.com",
            "role": "Developer"
        }
    ],
    "keywords": [
        "lanager",
        "lan",
        "party"
    ],
    "license": "AGPL-3.0",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ilumos/Providers"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "^7.0",
        "laravel/tinker": "^2.0",
        "laravel/socialite": "^4.3",
        "bandwidth-throttle/token-bucket": "^2.0",
        "davejamesmiller/laravel-breadcrumbs": "5.x",
        "doctrine/dbal": "^2.8",
        "graham-campbell/markdown": "^12.0",
        "league/csv": "^9.3",
        "socialiteproviders/steam": "dev-master",
        "syntax/steam-api": "^2.1"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5",
        "laravel/homestead": "^10",
        "barryvdh/laravel-debugbar": "dev-master",
        "barryvdh/laravel-ide-helper": "^2.7"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "Zeropingheroes\\Lanager\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan cache:clear",
            "@php artisan config:clear",
            "@php artisan view:clear",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
           "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ]
    }
}
